<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@id/test_panel_tv_tips_hvdcp"
        android:padding="10.0dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="114.0dip"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        style="@style/cit_comm_content_text" />
    <TextView
        android:id="@id/test_panel_tv_tips"
        android:padding="10.0dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15.0dip"
        android:layout_below="@id/test_panel_tv_tips_hvdcp"
        android:layout_centerHorizontal="true"
        style="@style/cit_comm_content_text" />
    <TextView
        android:id="@id/test_panel_tv_electrical"
        android:padding="10.0dip"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15.0dip"
        android:layout_below="@id/test_panel_tv_tips"
        android:layout_centerHorizontal="true"
        style="@style/cit_comm_content_text" />
    <TextView
        android:id="@id/test_panel_tv_charge_type"
        android:padding="10.0dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15.0dip"
        android:layout_below="@id/test_panel_tv_electrical"
        android:layout_centerHorizontal="true"
        style="@style/cit_comm_content_text" />
    <TextView
        android:id="@id/test_panel_tv_process"
        android:padding="10.0dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15.0dip"
        android:layout_below="@id/test_panel_tv_charge_type"
        android:layout_centerHorizontal="true"
        style="@style/cit_comm_content_text" />
</RelativeLayout>